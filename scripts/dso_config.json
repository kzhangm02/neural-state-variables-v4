         "on" : true
      },
      "const" : {
         "on" : true
      },
      "no_inputs" : {
         "on" : true
      },
      "uniform_arity" : {
         "on" : true
      },
      "soft_length" : {
         "loc" : 10,
         "scale" : 5,
         "on" : false
      },
      "domain_range" : {
         "on" : false
      }
   },

   "logging" : {
      "save_all_iterations" : false,
      "save_summary" : false,
      "save_positional_entropy" : false,
      "save_pareto_front" : true,
      "save_cache" : true,
